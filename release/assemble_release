echo Creating release file structure for $1
mkdir -p $1/GameData/BetterCrewAssignment
cp ../files/ModuleManager.*.dll $1/GameData
cp ../files/ModuleManagerLicense.md $1/GameData
cp ../files/changelog.txt $1/GameData/BetterCrewAssignment
cp ../files/*.cfg $1/GameData/BetterCrewAssignment
cp ../src/bin/Release/BetterCrewAssignment.dll $1/GameData/BetterCrewAssignment
cp ../LICENSE $1/GameData/BetterCrewAssignment
cp ../README.md $1/GameData/BetterCrewAssignment
echo Done.
